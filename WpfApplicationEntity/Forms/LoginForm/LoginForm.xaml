<Window x:Class="WpfApplicationEntity.Forms.LoginForm.LoginForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoginForm" Height="204" Width="207">
    <Grid>
        <TextBox x:Name="Log" HorizontalAlignment="Left" Height="23" Margin="72,31,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Pass" HorizontalAlignment="Left" Height="23" Margin="72,84,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Click" HorizontalAlignment="Left" Margin="57,128,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label HorizontalAlignment="Left" Margin="20,28,0,0" VerticalAlignment="Top">
            <Label Content="Login" RenderTransformOrigin="0.5,0.5">
                <Label.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="-17.819"/>
                        <RotateTransform/>
                        <TranslateTransform X="-4.179"/>
                    </TransformGroup>
                </Label.RenderTransform>
            </Label>
        </Label>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Height="23"/>

    </Grid>
</Window>
